<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root type="HBox" fx:id="mComponentPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label fx:id="mLabel" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" text="Label:" HBox.hgrow="NEVER" />
    <ToggleButton id="booleantoggle" fx:id="mBoolToggle" mnemonicParsing="false" onAction="#onToggled" text="True" HBox.hgrow="NEVER">
      <HBox.margin>
        <Insets bottom="3.0" right="3.0" top="3.0" />
      </HBox.margin>
    </ToggleButton>
    <Pane maxHeight="1.7976931348623157E308" prefHeight="-1.0" prefWidth="10.0" />
    <Button fx:id="mSave" mnemonicParsing="false" text="Save">
      <stylesheets>
        <URL value="@General.css" />
      </stylesheets>
    </Button>
  </children>
  <stylesheets>
    <URL value="@ArgumentView.css" />
    <URL value="@General.css" />
  </stylesheets>
</fx:root>
