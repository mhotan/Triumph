<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.*?>

<fx:root type="HBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns:fx="http://javafx.com/fxml">
  <children>
    <BorderPane fx:id="mComponentPane" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
      <center>
        <VBox fx:id="mCenter" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
      </center>
      <top>
        <HBox fx:id="mLabelPane" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" spacing="5.0" BorderPane.alignment="CENTER">
          <children>
            <Label fx:id="mLabel" maxHeight="1.7976931348623157E308" maxWidth="-1.0" prefHeight="-1.0" text="Title" HBox.hgrow="NEVER">
              <font>
                <Font name="System Bold" size="13.0" />
              </font>
            </Label>
            <Button fx:id="mAddElem" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onAddNewElem" prefWidth="-1.0" text="Add Element" />
            <Label fx:id="mError" maxHeight="1.7976931348623157E308" prefHeight="-1.0" text="Error" textFill="RED" HBox.hgrow="SOMETIMES" />
            <Pane maxHeight="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="mSave" mnemonicParsing="false" text="Save">
              <stylesheets>
                <URL value="@General.css" />
              </stylesheets>
            </Button>
          </children>
          <BorderPane.margin>
            <Insets />
          </BorderPane.margin>
        </HBox>
      </top>
    </BorderPane>
  </children>
  <stylesheets>
    <URL value="@ArgumentView.css" />
    <URL value="@General.css" />
  </stylesheets>
</fx:root>
