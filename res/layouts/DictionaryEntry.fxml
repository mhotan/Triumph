    <AnchorPane id="mKeyArg" fx:id="mKeyContainer" maxWidth="1.7976931348623157E308" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
    <AnchorPane id="mValArg" fx:id="mValContainer" maxWidth="1.7976931348623157E308" minHeight="-1.0" minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
    <Label fx:id="mError" maxWidth="1.7976931348623157E308" text="Label" textFill="RED" HBox.hgrow="NEVER" />
  </children>
  <padding>
    <Insets left="3.0" right="3.0" />
  </padding>
  <stylesheets>
    <URL value="@ArgumentView.css" />
  </stylesheets>
</fx:root>
