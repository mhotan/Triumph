<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" spacing="0.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <HBox prefHeight="-1.0" prefWidth="-1.0" styleClass="component-pane">
      <children>
        <Label fx:id="mLabel" prefHeight="25.0" text="Label" textAlignment="CENTER" textOverrun="CLIP" HBox.hgrow="NEVER" />
        <Pane maxHeight="1.7976931348623157E308" prefHeight="-1.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="mError" prefHeight="25.0" text="Error" textFill="RED" />
        <Pane maxHeight="1.7976931348623157E308" prefHeight="-1.0" prefWidth="10.0" />
        <Button fx:id="mSave" mnemonicParsing="false" text="Save">
          <stylesheets>
            <URL value="@General.css" />
          </stylesheets>
        </Button>
      </children>
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </HBox>
  </children>
  <padding>
    <Insets />
  </padding>
  <stylesheets>
    <URL value="@General.css" />
  </stylesheets>
</fx:root>
