<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="HBox" fx:id="mComponentPane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="400.0" styleClass="component-pane" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label fx:id="mLabel" alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" text="Label" HBox.hgrow="NEVER">
      <HBox.margin>
        <Insets left="3.0" />
      </HBox.margin>
    </Label>
    <TextField fx:id="mInput" onAction="#onSetCurrentValue" prefHeight="27.0" prefWidth="-1.0" promptText="Enter value or &lt;null&gt; for Null" HBox.hgrow="ALWAYS" />
    <Label fx:id="mError" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" styleClass="error-message" text="Error" textAlignment="RIGHT" textFill="RED" HBox.hgrow="NEVER">
      <HBox.margin>
        <Insets right="3.0" />
      </HBox.margin>
    </Label>
    <Button fx:id="mSave" mnemonicParsing="false" text="Save">
      <stylesheets>
        <URL value="@General.css" />
      </stylesheets>
    </Button>
  </children>
  <stylesheets>
    <URL value="@ArgumentView.css" />
    <URL value="@General.css" />
  </stylesheets>
</fx:root>
