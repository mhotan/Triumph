<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
  <children>
    <VBox layoutX="0.0" layoutY="0.0" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label alignment="CENTER" contentDisplay="TOP" maxWidth="1.7976931348623157E308" text="Result" textAlignment="RIGHT">
          <font>
            <Font name="System Bold" size="18.0" />
          </font>
          <VBox.margin>
            <Insets bottom="3.0" top="3.0" />
          </VBox.margin>
        </Label>
        <TextArea fx:id="mResultArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="-1.0" wrapText="true">
          <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </VBox.margin>
        </TextArea>
      </children>
    </VBox>
  </children>
</fx:root>
