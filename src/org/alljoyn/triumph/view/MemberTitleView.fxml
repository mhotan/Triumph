<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.BorderPane" xmlns:fx="http://javafx.com/fxml">
  <left>
    <VBox fx:id="mMainLabelPane" alignment="CENTER" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label fx:id="mMemberName" maxWidth="1.7976931348623157E308" styleClass="general-label, title-label" text="Method/Signature" />
        <Label id="mAnnotationsLabel" fx:id="mAnnotationTitleLabel" styleClass="general-label, annotation-title-label" text="Annotation(s):" />
        <ListView fx:id="mAnnotationsList" prefHeight="-1.0" prefWidth="-1.0" />
      </children>
    </VBox>
  </left>
  <right>
    <VBox fx:id="mDetailLabelPane" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label fx:id="mWellKnownNameLabel" maxWidth="1.7976931348623157E308" styleClass="general-label" text="Well Known Name" />
        <Label fx:id="mInterfaceLabel" maxWidth="1.7976931348623157E308" styleClass="general-label" text="Interface" />
        <Label fx:id="mObjectPathLabel" maxWidth="1.7976931348623157E308" styleClass="general-label" text="Object Path" />
      </children>
    </VBox>
  </right>
  <stylesheets>
    <URL value="@MemberTitle.css" />
  </stylesheets>
</fx:root>
