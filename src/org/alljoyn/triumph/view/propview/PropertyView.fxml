<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.*?>

<fx:root type="VBox" maxWidth="1.7976931348623157E308" styleClass="container" xmlns:fx="http://javafx.com/fxml">
  <children>
    <Label fx:id="mServiceName" maxWidth="1.7976931348623157E308" text="Service Name" />
    <Label fx:id="mInterfaceName" maxWidth="1.7976931348623157E308" text="Interface Name" />
    <Label fx:id="mObjectPath" maxWidth="1.7976931348623157E308" prefWidth="200.0" text="Object Path" />
    <Label fx:id="mName" alignment="BOTTOM_CENTER" maxWidth="1.7976931348623157E308" text="Property Name" />
    <Label fx:id="mAccessLabel" maxWidth="1.7976931348623157E308" styleClass="access_label" text="Access:" />
    <AnchorPane fx:id="mContent" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0" />
    <HBox fx:id="mButtonPane" alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Button fx:id="mSetButton" mnemonicParsing="false" onAction="#onSet" text="Set" HBox.hgrow="ALWAYS" />
        <Pane fx:id="mButtonSpaceFiller" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="-1.0" prefWidth="20.0" />
        <Button fx:id="mGetButton" mnemonicParsing="false" onAction="#onGet" text="Get" HBox.hgrow="ALWAYS" />
      </children>
    </HBox>
    <Label id="mErrorMessage" fx:id="mErrorLabel" alignment="TOP_CENTER" maxWidth="1.7976931348623157E308" styleClass="error-member" text="Unknown Error" textFill="RED" wrapText="true">
      <graphic>
        <Label id="mErrorTitle" alignment="BOTTOM_CENTER" maxWidth="1.7976931348623157E308" styleClass="error-member, genlabel" text="Error:" textFill="RED" wrapText="false">
          <font>
            <Font name="System Bold" size="16.0" />
          </font>
        </Label>
      </graphic>
    </Label>
  </children>
  <stylesheets>
    <URL value="@PropertyView.css" />
  </stylesheets>
</fx:root>
