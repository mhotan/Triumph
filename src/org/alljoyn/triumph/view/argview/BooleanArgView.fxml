<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.geometry.Insets?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns:fx="http://javafx.com/fxml">
	<children>
		<HBox  spacing="10.0"
			xmlns:fx="http://javafx.com/fxml">
			<children>
				<Label fx:id="mLabel" alignment="CENTER_RIGHT" prefHeight="27.0"
					prefWidth="43.0" text="Label" HBox.hgrow="NEVER" />
				<ToggleButton  id="booleantoggle" fx:id="mBoolToggle"
					mnemonicParsing="false" onAction="#onToggled" text="True"
					HBox.hgrow="NEVER">
					<HBox.margin>
						<Insets bottom="3.0" right="3.0" top="3.0" />
					</HBox.margin>
				</ToggleButton>
			</children>
		</HBox>
	</children>
</fx:root>

