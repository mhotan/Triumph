<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<fx:root type="javafx.scene.layout.VBox" spacing="0.0" xmlns:fx="http://javafx.com/fxml">
  <children>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Label fx:id="mLabel" prefHeight="25.0" text="Label" textAlignment="CENTER" textOverrun="CLIP" HBox.hgrow="NEVER" />
        <Button fx:id="mSave" mnemonicParsing="false" onAction="#onSave" text="Save" textOverrun="CLIP" HBox.hgrow="NEVER">
          <HBox.margin>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
          </HBox.margin>
        </Button>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="mError" prefHeight="25.0" text="Error" textFill="RED" />
      </children>
      <VBox.margin>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
      </VBox.margin>
    </HBox>
  </children>
  <padding>
    <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
  </padding>
</fx:root>
